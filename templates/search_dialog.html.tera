<!doctype html>
<html lang="en">
<head>
    <title>datavzrd report</title>
    <link rel="stylesheet" type="text/css" href="../../static/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../../static/bootstrap-table.min.css">
</head>

<body>
<script src="../../static/jquery.min.js"></script>
<script src="../../static/bootstrap.bundle.min.js"></script>
<script src="../../static/bootstrap-table.min.js"></script>

<div class="container-fluid">
    <div class="row">
        <div class="col-9 mr-auto">
            <table class="table table-hover" data-toggle="table" data-search="true" style="width: 100%; position: center">
                <thead>
                <tr>
                    <th>page</th>
                    <th>{{ title }}</th>
                </tr>
                </thead>
                <tbody>
                {% for record in records %}
                <tr>
                    <td><a target="_parent" href="../index_{{ record.1 }}.html?highlight={{ record.2 }}" style="display: table-cell">{{ record.1 }}</a></td>
                    <td>{{ record.0 }}</td>
                </tr>
                {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>
</body>
</html>
